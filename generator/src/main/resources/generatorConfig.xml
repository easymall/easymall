<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="easymall" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/easymall"
                        userId="root" password=""/>

        <javaModelGenerator targetPackage="com.zhu8fei.mybatis.model" targetProject="\generator\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.zhu8fei.mybatis.xml" targetProject="\generator\src\main\resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.zhu8fei.mybatis.mapper" targetProject="\generator\src\main\resources">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table schema="easymall" tableName="u_user" domainObjectName="User">
        </table>

    </context>
</generatorConfiguration>